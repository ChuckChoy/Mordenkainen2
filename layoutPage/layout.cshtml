<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mordenkainen2</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/bootstrap_css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/bootstrap_css/bootstrap-grid.min.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/bootstrap_css/bootstrap-reboot.min.css" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="~/css/site.css" />
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
